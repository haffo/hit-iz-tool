            <ByID ID="CWE_IZ">
                <Predicate ID="[CWE_IZ]3[1]" Target="3[1]" TrueUsage="R" FalseUsage="X">
                    <Description>If CWE.1 (Identifier) is valued.</Description>
                    <Condition>

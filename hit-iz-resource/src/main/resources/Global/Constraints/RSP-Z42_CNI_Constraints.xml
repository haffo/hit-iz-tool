                <Predicate ID="[ERL_IZ]4[1]" Target="4[1]" TrueUsage="R" FalseUsage="RE">
                    <Description>If ERL.5 (Component Number) is valued.</Description>
                    <Condition>
                        <Presence Path="5[1]"/>
